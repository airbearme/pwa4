# Production Environment Configuration
# This file should be used for live deployment with real credentials

# Stripe Configuration (Production keys - replace with real keys)
STRIPE_PUBLIC_KEY=pk_live_your_real_stripe_public_key
STRIPE_SECRET_KEY=sk_live_your_real_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_real_stripe_webhook_secret
VITE_STRIPE_PUBLIC_KEY=pk_live_your_real_stripe_public_key

# Supabase Configuration (Production keys - replace with real keys)
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_SECRET_KEY=your_supabase_service_role_key_for_production
VITE_SUPABASE_URL=https://your-project-ref.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_for_production
VITE_SUPABASE_PUBLISHABLE_KEY=your_supabase_publishable_key_for_production

# Application Configuration
NODE_ENV=production
PORT=5000

# Database Configuration
DATABASE_URL=postgresql://postgres.your_password@aws-0-us-east-1.pooler.supabase.com:6543/postgres

# Disable mock database for production
USE_MOCK_DATABASE=false

# Security Settings
SESSION_SECRET=generate_a_strong_random_secret_here_with_32+_characters
JWT_SECRET=generate_another_strong_random_secret_here_with_32+_characters

# CORS Configuration for production
ALLOWED_ORIGINS=https://airbear.me,https://www.airbear.me

# Rate Limiting
RATE_LIMIT_WINDOW_MS=15*60*1000
RATE_LIMIT_MAX=100

# Logging
LOG_LEVEL=info